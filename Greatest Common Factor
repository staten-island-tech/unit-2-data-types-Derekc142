def print_factors(x):
    factors_list = []
    for i in range (1, x + 1):
        if x % i == 0:
            factors_list.append(i)
        return factors_list
number1 = int(input("Number "))
factorsofnumber1 = print_factors(number1)
number2 = int(input("Number "))
factorsofnumber2 = print_factors(number2)
gcf = 2
for number in factorsofnumber1: 
    if number in factorsofnumber2 and gcf < number:
        gcf = number
print("The GCF of", number1, "and", number2, "are:", gcf)